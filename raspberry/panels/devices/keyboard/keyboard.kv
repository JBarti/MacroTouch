#:kivy 1.10.1

<Key>:
    size_hint_x: None
    size_hint_y: None
    width: 60
    height: 70
    
<Spacebar>:
    size_hint_x: None
    width: 450
    
<KeyRow>:
    size_hint_y: None
    size_hint_x: 1
    margin: None
    width: root.width
    spacing: 2
    height: self.minimum_height + 2

<Keyboard>:
    size_hint_y: None
    size_hint_x: 1
